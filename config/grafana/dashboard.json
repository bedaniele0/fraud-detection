{
  "dashboard": {
    "title": "Fraud Detection System - DVP-PRO",
    "tags": ["fraud-detection", "ml", "monitoring", "dvp-pro"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Fraud Rate (Real-time)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "fraud_detection_fraud_rate",
            "legendFormat": "Fraud Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Fraud Rate",
            "min": 0,
            "max": 1
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.01], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "60s",
          "handler": 1,
          "name": "High Fraud Rate Alert",
          "noDataState": "no_data"
        }
      },
      {
        "id": 2,
        "title": "Total Predictions",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "fraud_detection_predictions_total",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto",
          "orientation": "auto"
        }
      },
      {
        "id": 3,
        "title": "Fraud Detected (Today)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 18, "y": 0},
        "targets": [
          {
            "expr": "fraud_detection_fraud_count",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "auto"
        },
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "API Response Time (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(fraud_detection_api_latency_seconds_bucket[5m]))",
            "legendFormat": "p95 Latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(fraud_detection_api_latency_seconds_bucket[5m]))",
            "legendFormat": "p50 Latency",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Response Time"
          }
        ]
      },
      {
        "id": 5,
        "title": "Model Performance Metrics",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "fraud_detection_model_precision",
            "format": "table",
            "instant": true,
            "refId": "A"
          },
          {
            "expr": "fraud_detection_model_recall",
            "format": "table",
            "instant": true,
            "refId": "B"
          },
          {
            "expr": "fraud_detection_model_f1_score",
            "format": "table",
            "instant": true,
            "refId": "C"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "Value #A": "Precision",
                "Value #B": "Recall",
                "Value #C": "F1 Score"
              }
            }
          }
        ]
      },
      {
        "id": 6,
        "title": "Predictions by Risk Level",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "sum by (risk_level) (fraud_detection_predictions_by_risk)",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "pieType": "donut"
        }
      },
      {
        "id": 7,
        "title": "Data Drift (PSI Score)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "fraud_detection_drift_psi_score",
            "legendFormat": "{{feature}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "PSI Score"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.2], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "1h", "now"]},
              "reducer": {"params": [], "type": "max"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "300s",
          "handler": 1,
          "name": "Data Drift Detected",
          "noDataState": "no_data"
        }
      },
      {
        "id": 8,
        "title": "API Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "rate(fraud_detection_api_errors_total[5m])",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Error Rate"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [0.05], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "name": "High API Error Rate"
        }
      },
      {
        "id": 9,
        "title": "Model Threshold",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "fraud_detection_model_threshold",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 10,
        "title": "Model Version",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 24},
        "targets": [
          {
            "expr": "fraud_detection_model_version",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 11,
        "title": "System Uptime",
        "type": "stat",
        "gridPos": {"h": 4, "w": 6, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "time() - fraud_detection_start_time",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "unit": "s"
        }
      },
      {
        "id": 12,
        "title": "Requests per Minute",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "rate(fraud_detection_api_requests_total[1m]) * 60",
            "legendFormat": "Requests/min",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Requests/min"
          }
        ]
      },
      {
        "id": 13,
        "title": "False Positive Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "fraud_detection_false_positive_rate",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "max": 0.1,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.02},
                {"color": "red", "value": 0.05}
              ]
            },
            "unit": "percentunit"
          }
        }
      },
      {
        "id": 14,
        "title": "False Negative Rate",
        "type": "gauge",
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 28},
        "targets": [
          {
            "expr": "fraud_detection_false_negative_rate",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": false,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "max": 0.3,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 0.2}
              ]
            },
            "unit": "percentunit"
          }
        }
      }
    ],
    "templating": {
      "list": [
        {
          "current": {"text": "5m", "value": "5m"},
          "name": "interval",
          "options": [
            {"text": "1m", "value": "1m"},
            {"text": "5m", "value": "5m"},
            {"text": "15m", "value": "15m"},
            {"text": "1h", "value": "1h"}
          ],
          "query": "1m,5m,15m,1h",
          "type": "custom"
        }
      ]
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    }
  },
  "overwrite": false
}
